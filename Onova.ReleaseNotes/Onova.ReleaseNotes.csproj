<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net461;netcoreapp3.0;netcoreapp3.1;net5.0</TargetFrameworks>
    <Authors>dady8889</Authors>
    <Description>Onova extension that provides support for release notes.</Description>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>onova releasenotes changelog extension</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Onova" Version="2.6.2" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)' == 'net461'">
    <Reference Include="System.Net.Http" />
  </ItemGroup>
  
</Project>
